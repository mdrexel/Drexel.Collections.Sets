<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <LangVersion>8.0</LangVersion>
    <Nullable>enable</Nullable>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <RootNamespace>Drexel.Collections</RootNamespace>
  </PropertyGroup>

  <PropertyGroup>
    <Version>1.0.0</Version>
    <Authors>Max Drexel</Authors>
    <Description>Defines an IImmutableSet that implements IReadOnlySet.</Description>
    <Copyright>Copyright 2019</Copyright>
    <PackageLicenseUrl>https://github.com/ltnublet/Drexel.Collections.Sets/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/ltnublet/Drexel.Collections.Sets</PackageProjectUrl>
    <PackageTags>collection collections set sets immutable drexel</PackageTags>
    <PackageReleaseNotes>1.0.0: Initial release</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.Collections.Immutable" Version="1.5.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Drexel.Collections.Sets\Drexel.Collections.Sets.csproj" />
  </ItemGroup>

</Project>
